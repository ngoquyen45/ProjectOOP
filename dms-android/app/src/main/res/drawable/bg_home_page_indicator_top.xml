<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="16dp"
        android:right="5dp"
        android:top="4dp"
        android:bottom="14dp"
        >
        <shape android:shape="rectangle">
            <solid android:color="?colorPrimary"/>
        </shape>
    </item>

    <item
        android:left="10dp"
        android:right="29dp"
        android:top="3dp"
        android:bottom="-1dp">
        <rotate android:fromDegrees="40">
            <shape android:shape="rectangle">
                <solid android:color="?colorPrimary"/>
            </shape>
        </rotate>
    </item>

    <item
        android:left="0dp"
        android:right="30dp"
        android:top="18dp"
        android:bottom="0dp">
        <shape android:shape="rectangle">
            <solid android:color="?colorPrimary"/>
        </shape>
    </item>

    <item
        android:left="35dp"
        android:right="0dp"
        android:top="2dp"
        android:bottom="12dp">
        <shape android:shape="rectangle">
            <solid android:color="?colorPrimary" />
            <corners android:radius="2.5dp" />
        </shape>
    </item>

    <size android:width="40dp"
        android:height="19dp" />

</layer-list>